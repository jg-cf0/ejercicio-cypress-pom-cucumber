{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.js",
    "cypress/**/*.feature",
    "cypress/**/*.cy.js"
  ],
  "exclude": ["node_modules"]
}